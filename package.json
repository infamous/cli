{
	"name": "@lume/cli",
	"version": "0.10.1",
	"description": "A CLI for managing JavaScript/TypeScript packages.",
	"homepage": "https://github.com/lume/cli",
	"bin": {
		"lume": "bin/cli.js"
	},
